{
  "name": "nhk_api",
  "version": "0.1.0",
  "description": "Node.js client for NHK API",
  "main": "index.js",
  "scripts": {
    "test": "tape test/*.js",
    "build": "browserify -g uglifyify -r ./index.js:nhk_api -o client/nhk_api.js"
  },
  "keywords": [
    "nhk",
    "api"
  ],
  "author": "yosuke furukawa",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "tape": "~2.10.2",
    "moment": "~2.5.1",
    "uglifyify": "~1.2.3",
    "browserify": "~3.32.0"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/6..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/latest"
    ]
  }
